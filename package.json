{
  "name": "clean-to-the-core",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "deploy": "npm run docsDeploy && ng gh-pages:deploy",
    "deploy-gh-pages": "ng github-pages:deploy --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/app",
    "docsCommit": "git add docs  git commit -m \"Updating 'docs'.\" && git checkout master",
    "docsDeploy": "git checkout gh-pages && git checkout master -- docs && npm run docsCommit",
    "e2e": "protractor",
    "fixCoverage": "node ./scripts/fix_coverage.js",
    "lint": "tslint \"src/**/*.ts\"",
    "pree2e": "webdriver-manager update",
    "minCoverage": "node ./scripts/minimum_coverage.js",
    "start": "ng serve",
    "test": "ng lint && ng test --watch false && npm run fixCoverage",
    "typedoc": "typedoc"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional",
      "pre-commit": [
        "ng lint"
      ],
      "pre-push": [
        "rm -rf node_modules",
        "npm install",
        "ng test --watch false",
        "npm run fixCoverage",
        "npm run minCoverage"
      ]
    }
  },
  "private": false,
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/forms": "2.1.1",
    "@angular/http": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/router": "3.1.1",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-rc.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "angular-cli": "^1.0.0-beta.18",
    "codelyzer": "1.0.0-beta.2",
    "coveralls": "^2.11.14",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "lcov-parse": "0.0.10",
    "pre-git": "^3.10.0",
    "protractor": "4.0.9",
    "ts-node": "1.6.0",
    "tslint": "3.15.1",
    "typedoc-plugin-external-module-name": "^1.0.4",
    "typescript": "2.0.6"
  }
}
